  "keywords": [
    "eos",
    "tonarts",
    "contract",
    "eosjs",
    "scatter",
    "blockchain"
  ],
  "scripts": {
    "build": "webpack -p -o ./play/[name].bundle.js --devtool eval-source-map && webpack -p",
