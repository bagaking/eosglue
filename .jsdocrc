{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "source": {
        "include": [
            "./src",
            "./scatterBinder",
            "./Readme.md"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./doc/",
        "recurse": true
    }
}